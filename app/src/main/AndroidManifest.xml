<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.goalreminderbeta.sa.goalreminderbeta">

    <application
        android:name=".SugarOrmTestApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="DATABASE"
            android:value="goals.db" />
        <meta-data
            android:name="VERSION"
            android:value="2" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />

        <activity android:name=".all.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".all.AllSectionTheme"></activity>
        <activity android:name=".all.sport.WeightCorrectionActivity"></activity>
        <activity android:name=".all.sport.AllSubThemesSport"></activity>
        <activity android:name=".all.science.AllSubThemesScience"></activity>
        <activity android:name=".all.science.ReadBookActivity"></activity>
    </application>

</manifest>