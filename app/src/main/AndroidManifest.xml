<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.goalreminderbeta.sa.goalreminderbeta">

    <application
        android:name=".SugarOrmTestApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="DATABASE"
            android:value="goals.db" />
        <meta-data
            android:name="VERSION"
            android:value="2" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <uses-permission android:name="android.permission.VIBRATE"/>

        <activity android:name=".all.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".all.AllSectionTheme" />
        <activity android:name=".all.sport.WeightCorrectionActivity" />
        <activity android:name=".all.sport.RunCorrectionActivity" />
        <activity android:name=".all.sport.ElementCorrectionActivity" />
        <activity android:name=".all.sport.RepeatsCorrectionActivity" />
        <activity android:name=".all.sport.AllSubThemesSport" />
        <activity android:name=".all.science.AllSubThemesScience" />
        <activity android:name=".all.science.BookCorrectionActivity" />
        <activity android:name=".all.science.LanguageCorrectionActivity" />
        <activity android:name=".all.other.OtherCorrectionActivity" />
        <activity android:name=".all.RecordsActivity" />
        <activity android:name=".all.OptionsActivity" />
        <activity android:name=".all.RecordsActivity"></activity>
    </application>

</manifest>